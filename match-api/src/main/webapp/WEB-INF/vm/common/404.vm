<div class="w">
	<div class="wrapper">
		<div class="errorPage">
			<h1>404</h1>
			<span class="bubbles"></span>
			<div id="errorMsg">
				<span>糟糕！</span>
				<div style="display: inline-block">
					#if($!{data.errors} && $!{data.errors.size()}>0)
						#foreach($!{error} in $!{data.errors})
							<div>$!{error.msg}</div>
						#end
					#else
						<div>你请求的资源不存在！</div>
					#end
				</div>
			</div>
			<div class="cl"></div>
		</div>
	</div>
</div>